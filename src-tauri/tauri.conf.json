{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "bilidown_tauri",
  "version": "0.1.0",
  "identifier": "com.lingbingx.bilidown-tauri",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Bilibili Downloader",
        "width": 900,
        "height": 650,
        "minWidth": 900,
        "minHeight": 650,
        "resizable": true,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}